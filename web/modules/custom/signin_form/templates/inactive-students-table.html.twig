{#
/**
 * @file
 * Default theme implementation for displaying inactive students.
 *
 * Available variables:
 * - header: An array of header columns.
 * - rows: An array of rows, each containing an array of cell data.
 * - count: The count of inactive students.
 */
#}
{% if count > 0 %}
  <h2>{{ 'Inactive Students' }}</h2>
  <table>
    <thead>
      <tr>
        {% for column in header %}
          <th>{{ column }}</th>
        {% endfor %}
      </tr>
    </thead>
    <tbody>
      {% for row in rows %}
        <tr>
          {% for cell in row %}
            <td>{{ cell }}</td>
          {% endfor %}
        </tr>
      {% endfor %}
    </tbody>
  </table>
{% else %}
  <p>{{ 'No inactive students found.' }}</p>
{% endif %}
